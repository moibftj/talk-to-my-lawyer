{
  "name": "talk-to-my-lawyer",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.28.0",
  "scripts": {
    "predeploy:check": "node scripts/pre-deploy-check.js",
    "precommit": "pnpm lint && pnpm audit --audit-level=high",
    "hooks:install": "./scripts/install-git-hooks.sh",
    "build": "next build",
    "dev": "next dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "next start",
    "start:prod": "node scripts/validate-env.js && NODE_ENV=production next start -p ${PORT:-3000}",
    "health-check": "node scripts/health-check.js",
    "health-check:production": "./scripts/production-health-check.sh",
    "validate-env": "node scripts/validate-env.js",
    "validate-env:ci": "cp .env.ci .env && CI=true node scripts/validate-env.js && rm .env",
    "test:ci": "cp .env.ci .env && pnpm lint && pnpm audit --audit-level=high && pnpm build && CI=true node scripts/validate-env.js && rm .env",
    "db:migrate": "node scripts/run-migrations.js",
    "db:verify": "node scripts/verify-database-connection.js",
    "audit:security": "pnpm audit --audit-level=high",
    "update:packages": "pnpm update --latest",
    "autocommit": "git add -A && git diff --staged --quiet || (git commit -m \"Auto-commit: $(date)\" && git push origin main)",
    "commit": "git add -A && git commit -m",
    "push": "git push origin main",
    "save": "pnpm run commit -- \"Auto-save: $(date)\" && pnpm run push",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch"
  },
